{
  "name": "things-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "tsc -p tsconfig.json",
    "db:push": "drizzle-kit push:mysql",
    "db:studio": "drizzle-kit studio",
    "test": "node --no-warnings node_modules/ava/entrypoints/cli.mjs",
    "e2e": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "serve": "tsx watch main.ts --tsconfig ./tsconfig.json",
    "build": "tsup",
    "build:docker": "docker build . -t things-api:latest"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.362.0",
    "@aws-sdk/signature-v4-crt": "^3.354.0",
    "axios": "^1.4.0",
    "bformat": "latest",
    "cookie-parser": "^1.4.6",
    "cors": "~2.8.5",
    "crypted": "^0.1.1",
    "dotenv": "16.3.1",
    "drizzle-orm": "^0.28.5",
    "express": "~4.18.2",
    "express-rate-limit": "^6.8.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.5.2",
    "qs": "^6.11.2",
    "rate-limit-redis": "^3.0.2",
    "redis": "^4.6.7",
    "tslib": "^2.3.0",
    "uid": "^2.0.2",
    "zod": "^3.21.4"
