    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.9",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "ava": "^5.3.1",
    "drizzle-kit": "^0.19.13",
    "jest": "^29.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2"
  }
